
@{
    ViewBag.Title = "Create Client";
}

<h2>Create</h2>

<div class="container">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="card card-body ">
                <form>
                    <div class="form-group">
                        <label>Name</label>
                        <input id="txt_name" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label>Last Name</label>
                        <input id="txt_last_name" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label>Email</label>
                        <input id="txt_email" class="form-control" />
                    </div>
                    <div class="form-group">
                        <button type="button" class="btn btn-primary">Send</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

